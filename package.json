{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "description": "A Next.js project bootstrapped with create-next-app",
  "author": "TaeRok Park <qkrxofhr1@gmail.com> (https://github.com/taerok)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taerok/next-ts.git"
  },
  "homepage": "https://github.com/taerok/next-ts#readme",
  "bugs": {
    "url": "https://github.com/taerok/next-ts/issues",
    "email": "qkrxofhr1@gmail.com"
  },
  "keywords": [
    "next.js",
    "typescript"
  ],
  "engines": {
    "node": "^21"
  },
  "scripts": {
    "dev": "cross-env FORCE_COLOR=1 next dev -p 8002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "analyze": "cross-env ANALYZE=true yarn build",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "test-storybook": "cross-env FORCE_COLOR=1 test-storybook",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "coupling-graph": "npx madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|.yarn/|storybook-static/|jest.*.js|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.json' --image graph.svg",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.17",
    "@next/bundle-analyzer": "^14.0.1",
    "@t3-oss/env-nextjs": "^0.7.1",
    "git-conventional-commits": "^2.6.5",
    "madge": "^6.1.0",
    "next": "^14",
    "next-compose-plugins": "^2.2.1",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.39.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/nextjs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14",
    "eslint-config-prettier": "^9",
    "eslint-plugin-storybook": "^0.6.15",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "latest",
    "storybook": "^7.5.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.0.1"
}
